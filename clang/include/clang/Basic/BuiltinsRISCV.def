//
// This file defines the RISCV-specific builtin function database.  Users of
// this file must define the BUILTIN macro to make use of this information.
// Now only configured for YGJK
//
//===----------------------------------------------------------------------===//

// The format of this database matches clang/Basic/Builtins.def.
#if defined(BUILTIN) && !defined(TARGET_BUILTIN)
#   define TARGET_BUILTIN(ID, TYPE, ATTRS, FEATURE) BUILTIN(ID, TYPE, ATTRS)
#endif

TARGET_BUILTIN(__builtin_ygjk_rrrii, "LLiLLiLLi", "n", "riscv")
TARGET_BUILTIN(__builtin_ygjk_rrrpp, "LLiv*v*", "n", "riscv")
TARGET_BUILTIN(__builtin_ygjk_rrr, "LLi", "n", "riscv")

#undef BUILTIN
#undef TARGET_BUILTIN
